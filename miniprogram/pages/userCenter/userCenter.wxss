.h_title{
  width: 100%;
  height: 70rpx;
  position: fixed;
  top:0;
  /* border:1px solid; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding:0 20rpx;
  box-sizing: border-box;
  background-color:#fff;
  line-height: 70rpx;
  
}
.header{
  width:100%;
  background-color:#fff;
}
.headImg{
  display:flex;
  flex-direction: row;
}
.top{
  margin-top:100rpx;
  border-bottom:1px solid #888;
}
.bot{
  height: 100rpx;
  justify-content: space-around;
  line-height: 100rpx;
  margin-bottom:30rpx;
}
.h_img{
  border:1px solid;
  width:150rpx;
  height:150rpx;
  border-radius: 50%;
  margin:50rpx;
}
.username{
  margin-top:110rpx;
}
.lifeList{
  width:100%;
  background-color:#fff;
  display:flex;
  flex-direction: row;
  margin-bottom:30rpx;
}
.list{
  width:25%;
  border:1px solid;
  box-sizing: border-box;
  height:200rpx;
  text-align: center;
}
.listImg{
  display:block;
  width:70%;
  height:50%;
 margin:24rpx auto 14rpx;
}
.listTitle{
  font-size:28rpx;
}
.footer{
  width:100%;
  background-color:#fff;
}
.ele{
  width:100%;
  height:80rpx;
  display:flex;
  flex-direction: row;
  border-bottom:1px solid #eee;
}
.free{
  width:60rpx;
  height:60rpx;
  margin:10rpx;
}
.freeText{
 height: 80rpx;
line-height: 80rpx;
}
.righ{
position: absolute;
right:0;
}